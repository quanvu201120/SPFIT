<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment.MyPostFragment">

    <SearchView
        android:queryHint="Tiêu đề bài viết"
        android:id="@+id/searchView_MyPost"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="10dp"
        app:layout_constraintEnd_toStartOf="@+id/img_add_MyPost"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/img_add_MyPost"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="10dp"
        android:src="@drawable/icon_add"
        app:layout_constraintBottom_toBottomOf="@+id/searchView_MyPost"
        app:layout_constraintEnd_toStartOf="@+id/img_account_MyPost"
        app:layout_constraintTop_toTopOf="@+id/searchView_MyPost" />

    <ImageView
        android:id="@+id/img_account_MyPost"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="10dp"
        android:src="@drawable/icon_account"
        app:layout_constraintBottom_toBottomOf="@+id/img_add_MyPost"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/img_add_MyPost"
        app:layout_constraintVertical_bias="1.0" />

    <View
        android:id="@+id/kjlksjdfjjjljsd098"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/gray"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/searchView_MyPost" />
    <ListView
        android:id="@+id/listViewMyPost"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/kjlksjdfjjjljsd098" />



</androidx.constraintlayout.widget.ConstraintLayout>